{
  "name": "testeject",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react-app": "^3.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.3",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "express-async-wrap": "^1.0.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "isomorphic-fetch": "^2.2.1",
    "mf-response": "^1.0.0",
    "morgan": "^1.9.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-dev-utils": "^5.0.1",
    "react-dom": "^16.4.1",
    "react-scripts": "^2.0.0-next.3e165448",
    "resolve": "1.6.0",
    "serve": "^9.1.0",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack-manifest-plugin": "1.3.2",
    "webpack-node-externals": "^1.7.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "chalk": "^2.4.1",
    "concurrently": "^3.6.0",
    "eslint": "^3.19.0",
    "eslint-config-mf": "^1.1.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.39.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest-puppeteer": "^3.2.1",
    "jest-puppeteer-preset": "^3.2.1",
    "node-sass": "^4.9.1",
    "puppeteer": "^1.5.0",
    "sass-loader": "^7.0.3",
    "supertest": "^3.1.0",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "start:server": "nodemon --watch server server/index.js --exec babel-node",
    "start:dev": "concurrently --kill-others-on-fail 'yarn start:server' 'yarn start'",
    "build": "node scripts/build.js",
    "build:server": "webpack --config ./config/webpack.config.server.prod.js",
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "npm test -- --coverage",
    "test:e2e": "NODE_ENV=test jest -c end-to-end/jest.config.js",
    "serve": "node build/server/main.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "server/**/*.{js,jsx,mjs}",
      "!src/**/registerServiceWorker.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}",
      "<rootDir>/server/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "proxy": "http://localhost:3020"
}
